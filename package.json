{
  "name": "transferables",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "description": "Utility library that lists out all transferable objects that can be transfered between Workers and the main thread.",
  "umd": "Transferables",
  "access": "public",
  "legacy": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "@types/index.d.ts",
  "browser": "lib/index.mjs",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "default": "./lib/index.mjs"
    },
    "./lib/*": "./lib/*",
    "./src/*": "./src/*"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "@types": "./@types"
  },
  "files": [
    "lib",
    "src",
    "@types"
  ],
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "test:run": "vitest run tests",
    "build": "vite build",
    "benchmark:node": "vitest run ./benchmark/node.test.ts",
    "benchmark:browser": "echo \"Empty Benchmak\"",
    "benchmark:deno": "deno run --allow-run --allow-env --allow-read --allow-hrtime ./benchmark/deno.ts",
    "benchmark:deno:worker": "deno run --allow-run --allow-env --allow-read --allow-hrtime ./benchmark/deno.worker.ts",
    "benchmark:bun": "bun run vitest run --passWithNoTests ./benchmark/bun.test.ts",
    "benchmark": "pnpm benchmark:node && pnpm benchmark:deno && benchmark:deno:worker && pnpm benchmark:browser && pnpm benchmark:bun",
    "pre-release": "pnpm test:run && ultra benchmark && pnpm build",
    "semantic-release": "semantic-release"
  },
  "ultra": {
    "concurrent": [
      "pre-release",
      "benchmark"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "lib/**"
          ]
        }
      ]
    ]
  },
  "changelog": {
    "repo": "transferables",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal",
      "revert": ":rewind: Revert"
    },
    "cacheDir": ".changelog"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okikio/transferables.git"
  },
  "keywords": [
    "transferable",
    "typescript",
    "web worker",
    "shared worker",
    "worker",
    "structured clone",
    "arraybuffer",
    "messageport",
    "messagechannel",
    "readablestream",
    "writablestream",
    "transformstream",
    "streams"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okikio/transferables/issues"
  },
  "homepage": "https://github.com/okikio/transferables",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@stdlib/stats-base": "^0.0.7",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/web": "^0.0.81",
    "comlink": "^4.3.1",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "markdown-table": "^3.0.2",
    "pnpm": "^7.17.1",
    "pretty-bytes": "^6.0.0",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.3",
    "ultra-runner": "^3.10.5",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.25.3"
  }
}
